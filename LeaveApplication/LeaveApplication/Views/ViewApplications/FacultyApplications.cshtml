
@{
    ViewBag.Title = "FacultyApplications";
    Layout = "~/Views/Shared/EmpProfileLayOut.cshtml";
}

<script src="~/Scripts/jquery-3.3.1.min.js"></script>
<script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>
<script>

</script>
<div class="container-fluid">
    <div class="row vh-100 ">
        <div class="col">
            <h1  class="text-white" style=" margin-top:3%;margin-bottom:2% ;text-align:center">Faculty Applications</h1>
            <div class="card card-nav-tabs card-plain mb-0">
                <div class="card-header card-header-info">
                    <!-- colors: "header-primary", "header-info", "header-success", "header-warning", "header-danger" -->
                    <div class="nav-tabs-navigation">
                        <div class="nav-tabs-wrapper">
                            <ul class="nav nav-tabs" data-tabs="tabs">
                                <li class="nav-item">
                                    @Ajax.ActionLink("All", "FacultyAll", null, new AjaxOptions
 {
     HttpMethod = "GET",
          UpdateTargetId = "div1",
          InsertionMode = InsertionMode.Replace
 }, new { @class = "nav-link", data_toggle = "tab" })
                                </li>
                                <li class="nav-item">
                                    @Ajax.ActionLink("Pending", "FacultyPending", null, new AjaxOptions
{
    HttpMethod = "GET",
           UpdateTargetId = "div1",
           InsertionMode = InsertionMode.Replace
}, new { @class = "nav-link active", data_toggle = "tab" })
                                </li>
                                <li class="nav-item">
                                    @Ajax.ActionLink("Approved", "FacultyApproved", null, new AjaxOptions
 {
     HttpMethod = "GET",
             UpdateTargetId = "div1",
             InsertionMode = InsertionMode.Replace
 }, new { @class = "nav-link", data_toggle = "tab" })
                                </li>
                                <li class="nav-item">
                                    @Ajax.ActionLink("Rejected", "FacultyRejected", null, new AjaxOptions
{
   HttpMethod = "GET",
           UpdateTargetId = "div1",
           InsertionMode = InsertionMode.Replace
}, new { @class = "nav-link", data_toggle = "tab" })
                                </li>
                            </ul>
                        </div>
                    </div>

                </div>
            </div>
            <div id="div1">
                @Html.Action("FacultyPending")
            </div>


        </div>
    </div>
    </div>
